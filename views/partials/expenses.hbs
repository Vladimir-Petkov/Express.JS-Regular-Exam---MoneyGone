<table class="expenses">
    <thead>
        <tr>
            <th>Date</th>
            <th>Merchant</th>
            <th>Amount</th>
            <th>Category</th>
            <th>Description</th>
        </tr>

    </thead>
    <tbody>
        <tr>
            <td>
                <img src="./images/date.png" />
                <p>{{createdAt}}</p>
            </td>
            <td>
                <p>{{merchant}}</p>
                {{#if isChecked}}
                <div class="report">
                    <a href="/details/{{_id}}">Report</a>
                </div>
                {{/if}}
            </td>
            <td>
                <img src="./images/amount.png" alt="">
                <p>{{total}}</p>
            </td>
            <td>
                <img src="./images/category.png" alt="">
                <p>{{category}}</p>
            </td>
            <td>
                <img src="./images/hastag.png" alt="">
                <p>{{description}}</p>
            </td>
        </tr>

    </tbody>
</table>